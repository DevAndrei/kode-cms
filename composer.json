{
    "name": "kode-cms/kode-cms",
    "version": "0.0.1-dev",
    "type": "project",
    "license": "MIT",
    "description": "Symfony 4 CMS for developers from developers",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "homepage": "https://github.com/kode-cms/kode-cms",
    "support": {
        "issues": "https://github.com/kode-cms/kode-cms/issues"
    },
    "authors": [
        {
            "name": "KodeCMS",
            "homepage": "https://github.com/kode-cms/kode-cms/contributors",
            "role": "Community"
        },
        {
            "name": "k0d3r1s",
            "homepage": "https://github.com/k0d3r1s",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-iconv": "*",
        "nelmio/api-doc-bundle": "^3.2",
        "predis/predis": "^1.1",
        "snc/redis-bundle": "dev-master",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.0",
        "symfony/cache": "^4.0",
        "symfony/console": "^4.0",
        "symfony/finder": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/profiler-pack": "^1.0",
        "symfony/psr-http-message-bridge": "^1.0",
        "symfony/security-bundle": "^4.0",
        "symfony/swiftmailer-bundle": "^3.2",
        "symfony/templating": "^4.0",
        "symfony/translation": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "^4.0"
    },
    "require-dev": {
        "symfony/dotenv": "^4.0",
        "symfony/var-dumper": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "KodeCms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KodeCms\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01CBYSGSGVMHC8RG4DDEV96KK5",
            "allow-contrib": true
        }
    }
}
